    $input | & \\"node$exe\\"  \\"$basedir/src.env\\" $args
  } else {
    & \\"node$exe\\"  \\"$basedir/src.env\\" $args
  }
  $ret=$LASTEXITCODE
}
exit $ret
"
`;

